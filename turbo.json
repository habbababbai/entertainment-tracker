{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "dev": {
            "cache": false
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "build/**"]
        },
        "lint": {
            "dependsOn": ["^lint"],
            "outputs": []
        },
        "typecheck": {
            "dependsOn": ["^typecheck"]
        }
    }
}
