{
    "expo": {
        "name": "Entertainment Tracker",
        "slug": "entertainment-tracker",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#2563eb",
            "fullScreen": true
        },
        "ios": {
            "supportsTablet": false,
            "bundleIdentifier": "com.entertainmenttracker.app",
            "infoPlist": {
                "CFBundleDisplayName": "Entertainment Tracker"
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#2563eb"
            },
            "package": "com.entertainmenttracker.app",
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "scheme": "entertainmenttracker",
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        }
    }
}
